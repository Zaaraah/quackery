<h1>Admins#index</h1>
<p>Find me in app/views/admins/index.html.erb</p>

<table>

<% @admins.each do |a| %>
<tr> 
<td> <%= a.id %> </td>
<td> <%= a.username %> </td>
<td> <%= link_to 'Edit', edit_admin_path(a.id) %> </td>
<td> <%= link_to 'Delete', admin_path(a.id), method: :delete %> </td>
<td> <%= link_to 'Credentials info', a %> </td>
</tr>
<% end %>
</table>
<%= link_to 'Add', new_admin_path %>
